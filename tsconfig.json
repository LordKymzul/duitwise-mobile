{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "paths": {
      "@assets/*": [
        "./assets/*"
      ],
      "src/*": [
        "./src/*"
      ],
      "@core/*": [
        "./src/core/*"
      ],
      "@features/*": [
        "./src/features/*"
      ],
      "@types/*": [
        "./src/types/*"
      ]
    },
  }
}
